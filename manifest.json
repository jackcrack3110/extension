{
    "manifest_version":2,
    "name":"Coding Train Extension 2",
    "version":"2.0",
    "content_scripts":[
        {
            "matches":[
                "http://shiffman.net/*",
                "https://shiffman.net/*"
            ],
            "js": ["content.js"]
        }
    ],
    "background":{
        "scripts":["background.js"]
    },
    "browser_action":{
        "default_icon":"icon16.png"
    }
}